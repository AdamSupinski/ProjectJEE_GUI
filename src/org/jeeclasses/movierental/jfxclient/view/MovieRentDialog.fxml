<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="org.jeeclasses.movierental.jfxclient.controller.MovieRentDialogController">
    <children>
        <Label fx:id="titleLabel" alignment="CENTER" layoutX="17.0" layoutY="17.0" prefHeight="15.0" prefWidth="280.0"
               text="Wybierz datę zwrotu filmu">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <DatePicker fx:id="datePicker" editable="false" layoutX="58.0" layoutY="75.0"/>
        <Button fx:id="rentButton" defaultButton="true" layoutX="116.0" layoutY="135.0" mnemonicParsing="false"
                onAction="#handleRent" text="Wypożycz"/>
    </children>
</AnchorPane>
